{
  "name": "tamakoo",
  "version": "0.1.0",
  "scripts": {
    "build": "webpack --config ./webpack.config.dev.js",
    "dev": "webpack --config ./webpack.config.dev.js; node dev-server.js",
    "build:prod": "webpack --config ./webpack.config.prod.js",
    "flow": "flow",
    "flow-typed": "flow-typed install",
    "test:unit": "mocha --compilers js:babel-register ./src/polyfill.js",
    "test:all": "mocha --compilers js:babel-register ./src/polyfill.js ./polyfill.js **/*.spec.js",
    "start": "webpack-dev-server --inline --hot --colors --port 3341 ./webpack.config.dev.js",
    "server": "python dev-server.py",
    "server:prod": "python server.py"
  },
  "license": "MIT",
  "dependencies": {
    "date-utils": "^1.2.21",
    "material-ui": "^0.18.1",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "3.6.0",
    "velocity-react": "^1.3.3"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-power-assert": "1.0.0",
    "babel-preset-react": "6.24.1",
    "deep-equal": "1.0.1",
    "enzyme": "2.8.2",
    "express": "4.15.2",
    "fetch-mock": "5.10.0",
    "flow-bin": "0.45.0",
    "flow-typed": "2.1.2",
    "mocha": "3.3.0",
    "power-assert": "1.4.2",
    "react-test-renderer": "15.5.4",
    "sinon": "2.1.0",
    "svg-url-loader": "^2.0.2",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  }
}
